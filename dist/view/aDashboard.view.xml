<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:html="http://www.w3.org/1999/xhtml"
	controllerName="eventManagementEVA.controller.EventDashboard">
	<Page title="{i18n>adminDashboardTitle}" showNavButton="false" navButtonPress="onNavBack">
		<content>
			<Panel headerText="Admin Dashboard">
				<headerToolbar>
					<Toolbar>
						<Button text="Create Event" press="onBtnCreate" visible=""></Button>
					</Toolbar>
				</headerToolbar>
				<List items="{/}">
					<StandardListItem title="{LastName}" description="{FirstName}" type="Navigation" press="onListItemPress"/>
				</List>
			</Panel>
		</content>
	</Page>
</mvc:View>