{
    "_version": "1.7.0",
    "sap.app": {
        "id": "eventManagementEVA",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "dataSources": {
            "mainService": {
                "uri": "/destinations/M38/sap/opu/odata/sap/ZTEAM2_SRV/",
                "type": "OData",
                "settings": {
                    "oDataVersion": "2.0"
                }
            }
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "sourceTemplate": {
            "id": "ui5template.basicSAPUI5ApplicationProject",
            "version": "1.40.12"
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        },
        "supportedThemes": [
            "sap_hcb",
            "sap_belize"
        ]
    },
    "sap.ui5": {
        "rootView": {
            "viewName": "eventManagementEVA.view.App",
            "type": "XML"
        },
        "dependencies": {
            "minUI5Version": "1.30.0",
            "libs": {
                "sap.ui.core": {},
                "sap.m": {},
                "sap.ui.layout": {},
                "sap.ushell": {},
                "sap.collaboration": {},
                "sap.ui.comp": {},
                "sap.uxap": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "eventManagementEVA.i18n.i18n"
                }
            },
            "": {
                "dataSource": "mainService",
                "settings": {
                    "metadataUrlParams": {
                        "sap-documentation": "heading"
                    },
                    "useBatch": false,
                    "defaultCountMode": "Inline",
                    "defaultOperationMode": "Client",
                    "defaultUpdateMethod": "PUT"
                },
                "preload": true
            }
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewPath": "eventManagementEVA.view",
                "viewType": "XML",
                "controlId": "appControl",
                "controlAggregation": "pages",
                "bypassed": {
                    "target": "notFound"
                },
                "async": true
            },
            "routes": [
                {
                    "pattern": "",
                    "name": "login",
                    "target": "login"
                },
                {
                    "pattern": "EventDashboard/{userID}/{uRole}/{adminEmailAddress}/{nameUser}",
                    "name": "EventDashboard",
                    "target": "EventDashboard"
                },
                {
                    "pattern": "EventDetails/{eventID}/{userID}/{uRole}/{nameUser}",
                    "name": "EventDetails",
                    "target": "EventDetails"
                },
                {
                    "pattern": "EventCreate/{adminEmailAddress}/{nameUser}",
                    "name": "EventCreate",
                    "target": "EventCreate"
                }
            ],
            "targets": {
                "login": {
                    "viewName": "Login",
                    "viewId": "login",
                    "viewLevel": 0
                },
                "EventDashboard": {
                    "viewName": "EventDashboard",
                    "viewId": "EventDashboard",
                    "viewLevel": 1
                },
                "EventDetails": {
                    "viewName": "EventDetails",
                    "viewId": "EventDetails",
                    "viewLevel": 2
                },
                "EventCreate": {
                    "viewName": "EventCreate",
                    "viewId": "EventCreate",
                    "viewLevel": 3
                }
            },
            "resources": {
                "css": [
                    {
                        "uri": "css/style.css"
                    }
                ]
            }
        }
    },
    "sap.platform.hcp": {
        "uri": "",
        "_version": "1.1.0"
    }
}