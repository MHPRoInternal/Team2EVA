<mvc:View controllerName="eventManagementEVA.controller.Statistics" xmlns="sap.ui.table" xmlns:mvc="sap.ui.core.mvc"
	xmlns:u="sap.ui.unified" xmlns:c="sap.ui.core" xmlns:m="sap.m" height="100%">
	<m:Page id="statisticsPage">
		<m:customHeader class="headerContent">
			<m:Toolbar>
				<m:Button type="Back" press="onNavBack"/>
				<m:Image src="/webapp/resources/mhplogo.jpg" height="4.5em" width="9em" densityAware="true"></m:Image>
				<m:ToolbarSpacer/>
				<m:Label id="nameLabelStats"></m:Label>
				<m:MenuButton icon="sap-icon://action-settings" type="Accept" defaultAction="onDefaultActionAccept" useDefaultActionOnly="true">
				<m:menu>
					<m:Menu id="menuButton" itemSelected="onMenuAction">
						<m:items>
							<m:MenuItem id="changePasswordItem" text="Change Password" icon="sap-icon://private"/>
							<m:MenuItem id="logOutBtn" text="Logout"/>
						</m:items>
					</m:Menu>
				</m:menu>
			</m:MenuButton>
		</m:Toolbar>
	</m:customHeader>
	<m:Panel id="tableContent">
	</m:Panel>
</m:Page>
</mvc:View>