<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout" xmlns="sap.m"
	xmlns:html="http://www.w3.org/1999/xhtml" controllerName="eventManagementEVA.controller.aDashboard">
	<Page title="{i18n>DashTitle}">
		<headerContent>
			<Button id="createEventBtn" visible="false" width="10rem" text="Create Event" type="Unstyled" press="onCreatePress"></Button>
		</headerContent>
		<content>
				<FlexBox items="{toUserEvent}">
					
					<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="{Title}" press="handlePress">
						<TileContent >
							<ImageContent src="{Picture}"/>
						</TileContent>
					</GenericTile>
				</FlexBox>
		</content>
	</Page>
</mvc:View>